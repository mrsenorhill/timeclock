<h1>Clock In!</h1>
<div class="form">
<%= form_for :post, url: posts_path do |f| %>  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>
 
  <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </p>

    <p>
    <%= f.label :grade %><br>
    <%= f.text_field :grade %>
  </p>
 
  <p>
    <%= f.submit "Clock In"%>
  </p>
<% end %>
</div>